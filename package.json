{
  "name": "name",
  "version": "0.0.0",
  "packageManager": "pnpm@6.32.3",
  "description": "ts template",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "types/index.d.ts",
  "keywords": [
    "TypeScript",
    "template",
    "cjs",
    "mjs",
    "umd"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "typecheck": "tsc --noEmit",
    "fix": "eslint --fix --ext .js,.ts src tests",
    "prepublishOnly": "nr build"
  },
  "files": [
    "dist/",
    "types/"
  ],
  "author": "cyly cp786156072@163.com",
  "homepage": "https://github.com/ChpShy/[name]#readme",
  "bugs": {
    "url": "https://github.com/ChpShy/[name]/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChpShy/[name].git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./types/index.d.ts"
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.2",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@vitest/ui": "^0.7.4",
    "bumpp": "^7.1.1",
    "c8": "^7.11.0",
    "esbuild": "^0.14.27",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "rollup": "^2.70.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "typescript": "^4.1",
    "vite": "^2.8.6",
    "vitest": "^0.7.4"
  }
}